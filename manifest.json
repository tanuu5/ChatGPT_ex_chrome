{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.3",
  "description": "__MSG_extensionDescription__",
  "default_locale": "ja",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "chatgpt.js"
      ]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage",
    "https://api.openai.com/"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}