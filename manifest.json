{
    "manifest_version": 2,
    "name": "ChatGPT 拡張機能",
    "version": "1.0",
       "description": "拡張機能の説明文",
    "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["chatgpt.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": ["storage", "activeTab", "http://*/*", "https://*/*"]
}
